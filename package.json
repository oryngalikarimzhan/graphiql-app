{
  "homepage": "https://github.com/oryngalikarimzhan/graphiql-app",
  "name": "graphiql-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src --max-warnings=0",
